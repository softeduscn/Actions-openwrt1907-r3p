
config sysmonitor 'sysmonitor'
	option enable '1'
	option time '10'
	option homeip '192.168.1.1'
	option vpnip '192.168.1.110'
	option ddns '0'
	option smartdns '0'
	option smartdnsPORT '53'
	option smartdnsAD '0'
	option ipsec '0'
	option pptp '0'
	option firmware 'https://github.com/softeduscn/Actions-openwrt1907-r3p/releases/download/MI-R3P/openwrt-ramips-mt7621-xiaomi_mir3p-squashfs-sysupgrade.bin'
	option vpns '0'
	option vpnp '0'
	list dns '119.29.29.29'
	list dns '223.5.5.5'
	list vpn '192.168.1.110'
	list vpn '192.168.1.220'

